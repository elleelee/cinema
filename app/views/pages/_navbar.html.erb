  <!--     THE ONE WHERE YOU ARE SIGNED IN ALREADY AND CAN SIGN OUT -->
  <div class="navbar navbar-expand-sm navbar-light navbar-cinema">
    <a class="navbar-brand" href="/">
      <!-- <img src="./assets/images/newlogo.png"> -->
      <%= image_tag 'newlogo.png' %>
    </a>
    <% if params[:action] != 'home' && params[:action] != 'new' && params[:action] != 'edit' %>
      <%= render './events/searchnav' %>
    <% end %>
    <div></div>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav">
        <% if user_signed_in?  %>
          <a class="nav-link host-link" href="/events/new">Host an Event</a>
          <li class="nav-item dropdown">
            <div class="profile-face">
              <img class="avatar dropdown-toggle" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTzEMhHodV0WP-Afht-ukYLRG2JGLPQ539rimznsbZkSZm4XjzpHQ" />
            </div>
            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
              <a class="dropdown-item" id="profile-link" href="/profile">My Profile</a>
              <%= link_to 'Sign out', destroy_user_session_path, method: :delete, class: 'dropdown-item' %>
            </div>
          </li>
        <% else %>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
              <a class="nav-link host-link" href="/users/sign_up">Sign Up</a>
              <a class="nav-link host-link" href="/users//sign_in">Log In</a>
            </ul>
          </div>
        <% end %>
      </ul>
    </div>
  </div>
<!--       THE ONE WHERE YOU ARE SIGNED OUT AND WANT TO SIGN IN -->


